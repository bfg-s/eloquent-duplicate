{
    "name": "bfg/eloquent-duplicate",
    "description": "Duplicate any Eloquent model along with its underlying relationships",
    "license": "MIT",
    "keywords": [
        "laravel",
        "duplicate",
        "replicate",
        "copy",
        "eloquent",
        "model",
        "relations"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dzung Nguyen",
            "email": "dungnh@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Xsaven",
            "email": "bfg.script@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/contracts": "^8.0 || ^9.0",
        "illuminate/support": "^8.0 || ^9.0",
        "illuminate/database": "^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Duplicate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bfg\\Duplicate\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
